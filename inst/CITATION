year <- sub("-.*", "", meta$Date)
note <- sprintf("R package version %s", meta$Version)

bibentry(bibtype = "Manual",
         title = "iNEXT: iNterpolation and EXTrapolation for species diversity",
         author = "T. C. Hsieh and K. H. Ma and Anne Chao",
         year = year,
         note = note,
         url = "http://chao.stat.nthu.edu.tw/blog/software-download/")